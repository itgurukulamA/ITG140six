package com.example.demo.jdbcTemplate;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

@Service
public class jdbcTemplateService {
	@Autowired
	jdbcTemplate jt;
	public int insert(jdbcTemplate j) {
		int id=j.getId();
		String name=j.getName();
		int age=j.getAge();
		int regno=j.getRegno();
		String branch=j.getBranch();
		
		String a="insert into itg140(id,name,age,regno,branch)values(?,?,?,?,?)";
		return jt.update(a,id,name,age,regno,branch);
	}

}
